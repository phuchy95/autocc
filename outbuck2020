                  try{if(document.getElementsByClassName("border-btn")[i].innerHTML.includes("Music")){
                      document.getElementsByClassName("border-btn")[i].click();
                      break;
                  } else if (document.getElementsByClassName("border-btn")[i].innerHTML.includes("Entertainment")){
                      document.getElementsByClassName("border-btn")[i].click();
                      break
                  } else if (document.getElementsByClassName("border-btn")[i].innerHTML.includes("Technology")){
                      document.getElementsByClassName("border-btn")[i].click();
                      break
                  } else if (document.getElementsByClassName("border-btn")[i].innerHTML.includes("Gaming")){
                      document.getElementsByClassName("border-btn")[i].click();
                      break
                  } else if (document.getElementsByClassName("border-btn")[i].innerHTML.includes("Education")){
                      document.getElementsByClassName("border-btn")[i].click();
                      break
                  } else if (document.getElementsByClassName("border-btn")[i].innerHTML.includes("None")){
                      document.getElementsByClassName("border-btn")[i].click();
                      break
                  } else if (document.getElementsByClassName("border-btn")[i].innerHTML.includes("People")){
                      document.getElementsByClassName("border-btn")[i].click();
                      break
                  }
                     }catch(e){}
              }
          }},5000)
  }
  if(window.location.href.includes("video") == false){
