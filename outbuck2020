              document.getElementsByClassName("crcle-num sel-numb")[3].click();
              document.getElementsByClassName("crcle-num sel-numb")[4].click();
              document.getElementsByClassName("crcle-num sel-numb")[1].click();
              //document.getElementsByClassName("submit-btn")[0].click();
              stop1 = true;
          }
          if(document.getElementsByClassName("input-circle").length == 5 && stop1 == false && parseInt(document.getElementsByClassName("timer-count")[0].innerHTML) > 5){
              document.getElementsByClassName("crcle-num sel-numb")[6].click();
              document.getElementsByClassName("crcle-num sel-numb")[3].click();
              document.getElementsByClassName("crcle-num sel-numb")[4].click();
              document.getElementsByClassName("crcle-num sel-numb")[1].click();
              document.getElementsByClassName("crcle-num sel-numb")[2].click();
              //document.getElementsByClassName("submit-btn")[0].click();
              stop1 = true;
          }
          if(document.getElementsByClassName("input-circle").length == 6 && stop1 == false && parseInt(document.getElementsByClassName("timer-count")[0].innerHTML) > 5){
              document.getElementsByClassName("crcle-num sel-numb")[6].click();
              document.getElementsByClassName("crcle-num sel-numb")[3].click();
              document.getElementsByClassName("crcle-num sel-numb")[4].click();
              document.getElementsByClassName("crcle-num sel-numb")[1].click();
              document.getElementsByClassName("crcle-num sel-numb")[2].click();
              document.getElementsByClassName("crcle-num sel-numb")[7].click();
              document.getElementsByClassName("submit-btn")[0].click();
              stop1 = true;
          }
          if(document.getElementsByClassName("input-circle").length == 7 && stop1 == false && parseInt(document.getElementsByClassName("timer-count")[0].innerHTML) > 5){
              document.getElementsByClassName("crcle-num sel-numb")[6].click();
              document.getElementsByClassName("crcle-num sel-numb")[3].click();
              document.getElementsByClassName("crcle-num sel-numb")[4].click();
