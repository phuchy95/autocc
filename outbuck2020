(function() {
  if(window.location.href == "https://www.outbuck.com/invite/inflow"){
      document.getElementsByClassName("share-right-button share-button messanger")[0].click();
  }
  if(window.location.href.includes("videos")){
      setInterval(function(){
          if(parseInt(document.getElementsByClassName("button class-for-instant-baymack-videos header-button")[0].innerHTML) >= 4){
              window.location.href = "https://www.outbuck.com/"
          }
      },5000)
      setInterval(function(){
          if(document.getElementsByClassName("video-category video-category-questions-div")[0].style.display == "block"){
